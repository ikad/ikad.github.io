{
  "name": "ts-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "2.15.1",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "yarn run build:before && react-scripts-ts start",
    "build": "yarn run build:before && react-scripts-ts build && yarn run build:after",
    "build:before": "rm index.html & rm asset-manifest.json & rm manifest.json & rm favicon.ico & rm service-worker.js & rm -fr static",
    "build:after": "mv build/* . && rmdir build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.5",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.1"
  }
}
