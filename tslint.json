{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "semicolon": [true, "never"],
    "quotemark": [true, "double", "avoid-escape"],
    "no-console": [false],
    "no-debugger": [false]
  }
}
